{
  "realm": "social",
  "enabled": true,
  "registrationAllowed": true,
  "registrationEmailAsUsername": true,
  "verifyEmail": false,
  "ssoSessionIdleTimeout": 2592000,
  "ssoSessionMaxLifespan": 31536000,
  "clients": [
    {
      "clientId": "social-client",
